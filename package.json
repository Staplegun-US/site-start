{
  "name": "site-start",
  "version": "2.0.0-beta1",
  "author": "staplegun.us",
  "descripton": "A Boilerplate starter-kit for starting development right - the Staplegun way. Includes build automation and common CSS and JS libraries",
  "homepage": "https://github.com/Staplegun-US/site-start",
  "repository": {
    "type": "git",
    "url": "https://github.com/Staplegun-US/site-start"
  },
  "bugs": {
    "url": "https://github.com/Staplegun-US/site-start/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.17.1",
    "hamburgers": "^0.7.0",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "jquery": "^3.1.1",
    "livereload": "^0.6.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "postcss-clearfix": "^1.0.0",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-svg": "^1.0.6",
    "postcss-svgo": "^2.1.6",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.1.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  },
  "scripts": {
    "build:css:dev": "postcss -c postcss.dev.js -m",
    "build:css:prod": "postcss -c postcss.prod.js -m",
    "build:js:dev": "rollup -c rollup.config.dev.js --watch",
    "build:js:prod": "rollup -c rollup.config.prod.js",
    "build:images:dev": "imagemin src/images/* --out-dir=web/public/images",
    "reload": "livereload 'web/public/, web/craft/templates/'",
    "watch": "npm-run-all -p build:images:dev build:js:dev build:css:dev reload",
    "prod": "npm-run-all -p build:js:prod build:css:prod"
  },
  "private": "true",
  "license": "MIT"
}
